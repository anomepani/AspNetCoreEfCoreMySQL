@model IEnumerable<AspNetMySQL.Models.Questions>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Eid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Qid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Qns)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Choice)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sn)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Eid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Qid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Qns)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Choice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sn)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
